echo '<html>
  <head>
    <meta charset="utf-8">
    <link rel="stylesheet" type="text/css" href="style.css">
  </head>
  <body>
    <header>vall.su</header>
    <div id="main">' > ~/site/cooked/links.html

sed '/^http/ s:.*:<a href="&" target="_blank">&</a>:' ~/site/raw/links.txt | \
sed 's/$/<br>/' >> ~/site/cooked/links.html

echo '</div>
    <footer></footer>
  </body>
</html>' >> ~/site/cooked/links.html
